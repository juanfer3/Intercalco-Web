<p id="notice"><%= notice %></p>

<h1>Tintas</h1>

<table class="highlight">
  <thead>
    <tr>
      <th>Cod</th>
      <th>Linea uv</th>
      <th>Pantone</th>
      <th>Malla</th>
      <th>Producto</th>
      <th>Tipo de unidad</th>
      <th>Cantida</th>
      <th>Mezcla total</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tintas.each do |tinta| %>
      <tr>
        <td><%= tinta.cod %></td>
        <td><%= tinta.linea_uv.tipo_de_linea %></td>
        <td><%= tinta.pantone %></td>
        <td><%= tinta.malla.num_malla %></td>
        <td><%= tinta.producto.caracteristica%></td>
        <td><%= tinta.tipo_de_unidad %></td>
        <td><%= tinta.cantida %></td>
        <td><%= tinta.mezcla_total %></td>
        <td><%= link_to 'Show', tinta %></td>
        <td><%= link_to 'Edit', edit_tinta_path(tinta) %></td>
        <td><%= link_to 'Destroy', tinta, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


